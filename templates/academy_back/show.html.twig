{% extends "academy_back/base.html.twig" %}

{% block body %}
    <h1>Academy</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ academy_back.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ academy_back.name }}</td>
            </tr>
            <tr>
                <th>Category</th>
                <td>{{ academy_back.category }}</td>
            </tr>
            <tr>
                <th>Coach</th>
                {% for coach in coaches %} 
                <td>{{ coach.name }}</td>
                {% endfor %}
            </tr>
        </tbody>
    </table>
    
    {{ include('academy_back/btnedit.html.twig') }}

    {{ include('academy_back/_delete_form.html.twig') }}
    <a href="{{ path('app_academy_back_index') }}"><img src="{{ asset ('rami/public/icons/back.png') }}" alt="Edit" width="25" height="25">back</a>

  
{% endblock %}
