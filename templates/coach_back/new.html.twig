{% extends "academy_back/base.html.twig" %}

{% block body %}
    <h1>Add new Coach</h1>
    <div>
        {{ form_start(form,{'attr' : {'novalidate' : 'novalidate'}}) }}
        <table>
            <tr>
                <td>
                    {{ form_label(form.name,"Name : ") }}
                </td>
                <td>
                    {{ form_widget(form.name) }}
                </td>
                <td>
                    {{ form_errors(form.name) }}
                </td>
            </tr>
            <tr>
                <td>
                    {{ form_label(form.email,"Email : ") }}
                </td>
                <td>
                    {{ form_widget(form.email) }}
                </td>
                <td>
                    {{ form_errors(form.email) }}
                </td>
            </tr>
            <tr>
                <td>
                    {{ form_label(form.telephone,"Telephone : ") }}
                </td>
                <td>
                    {{ form_widget(form.telephone) }}
                </td>
                <td>
                    {{ form_errors(form.telephone) }}
                </td>
            </tr>
            <tr>
                <td>
                    {{ form_label(form.academy,"Academy : ") }}
                </td>
                <td>
                    {{ form_widget(form.academy) }}
                </td>
                <td>
                    {{ form_errors(form.academy) }}
                </td>
            </tr>
            <tr>
                <td>
                    {{ include('coach_back/_form.html.twig') }}
                <td>
            </tr>
            <tr>
                <td>
                    <a href="{{ path('app_coach_back_index') }}"><img src="{{ asset ('rami/public/icons/back.png') }}" alt="Edit" width="25" height="25">back</a>
                <td>
            </tr>
        </table>
        {{ form_end(form) }}
    </div>


   
{% endblock %}
