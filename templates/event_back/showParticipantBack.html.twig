{% extends 'back/base.html.twig' %}


{% block body %}
    <h1>Participants</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <th>Email</th>
            <th>Prenom</th>
            <th>Nom</th>
            <th>Telephone</th>
        </tr>
        {% for participant in participants %}
            <tr>
                <td>{{ participant.id }}</td>
                <td>{{ participant.email }}</td>
                <td>{{ participant.firstname }}</td>
                <td>{{ participant.lastname }}</td>
                <td>{{ participant.phone }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_event_back') }}"><button class="btn btn-outline-primary">Back</button></a>


{% endblock %}
