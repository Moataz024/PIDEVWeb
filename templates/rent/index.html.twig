{% extends "back/base.html.twig" %}



{% block body %}
    <h1>Rent index</h1>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons"
    rel="stylesheet">
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>RentAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for rent in rents %}
            <tr>
                <td>{{ rent.id }}</td>
                <td>{{ rent.rentAt ? rent.rentAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <button class="button button1" > <a href="{{ path('app_rent_show', {'id': rent.id}) }}">show <span class="material-icons">
                        visibility
    
                        </span></a>
                    </button>
                    <button class="button button1" > <a href="{{ path('app_rent_edit', {'id': rent.id}) }}">edit<span class="material-icons">
                        edit
                        </span></a>
                    </button>  
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    {# <a href="{{ path('app_rent_new') }}">Create new</a> #}
{% endblock %}
